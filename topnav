<script>
  require(['jquery', 'jquery/ui'], function ($) {
    //extra
    let acc_container_var = $('.acc_navigation_container');
    let acc_container_height = 0;
    acc_container_var.css('height', 'auto');
    acc_container_height = acc_container_var.height() + 'px';
    acc_container_var.css('height', '');
    //console.log('Navigation header magass√°ga: ' + acc_container_height);
    $('body').get(0).style.setProperty('--acc-cont-height', acc_container_height);

    $(window).on('resize', function () {
      acc_container_var.css('height', 'auto');
      acc_container_height = acc_container_var.height() + 'px';
      acc_container_var.css('height', '');
      console.log('Navigation header magass√°ga: ' + acc_container_height);
      $('body').get(0).style.setProperty('--acc-cont-height', acc_container_height);
    });

    $('.categories_btn').on('click', function toggleNavigation() {
      $('body').toggleClass('noscroll');
      $('.acc_navigation_container').toggleClass('active');
      $('.categories_btn').toggleClass('clicked');
    });

    $('.categories_btn_desktop').on('click', function toggleNavigation() {
      if ($(this).hasClass('clicked')) {
      } else {
        $('.overlay').fadeIn(325);
        $('.categories_btn_desktop span').html('Zav≈ô√≠t');
        $('.acc_navigation_container').addClass('active', 325, 'easeInOutSine');
        $('.acc_navigation_bar .acc_breadcrumbs .breadcrumbs').show();
        $('.acc_navigation_bar').addClass('active', 325, 'easeInOutSine');
        $('.categories_btn_desktop').addClass('clicked');
      }
    });

    const closeDropdown = function () {
      if ($('.acc_navigation_container').hasClass('active')) {
        $('.acc_navigation_container').removeClass('active', 100, 'easeInOutSine');
        $('.acc_navigation_bar').removeClass('active', 100, 'easeInOutSine');
        $('.acc_navigation_bar .acc_breadcrumbs .breadcrumbs').hide();
        $('.categories_btn_desktop').removeClass('clicked');
        $('.categories_btn_desktop span').html('Proch√°zet kategorie');
        //extra
        $('.overlay').fadeOut(325);
      }
    };

    $('.overlay').click(closeDropdown);
    $('.categories_btn_desktop').click(closeDropdown);
  });
</script>
<!--MINIFY NAVIGATION AND SHOW WHEN PAGE IS SCROLLED-->
<script>
  require(['jquery', 'jquery/ui', 'blazy'], function ($) {
    $(document).ready(function () {
      $('.acc_navigation_bar').css('top', '');
    });

    $(window).resize(isIntoView);

    function isIntoView(elem) {
      var pageTop = parseInt($('.page-wrapper').css('padding-top'), 10);
      var visibleTop = pageTop - 16;
      var documentViewTop = $(window).scrollTop() + visibleTop;
      console.log(documentViewTop);

      var documentViewBottom = documentViewTop + $(window).height();

      var elementTop = $(elem).offset().top;
      var elementBottom = elementTop + $(elem).height();

      return elementBottom <= documentViewBottom && elementBottom >= documentViewTop;
    }

    $(document).ready(function () {
      if (isIntoView($('.acc_navigation_list'))) {
        $('.acc_navigation_bar').addClass('hidden_offscreen');
      } else {
        $('.acc_navigation_bar').removeClass('hidden_offscreen');
      }
    });

    $(window).scroll(function () {
      if ($('.acc_navigation_container').hasClass('active')) {
      } else {
        if (isIntoView($('.acc_navigation_list'))) {
          $('.acc_navigation_bar').addClass('hidden_offscreen');
        } else {
          $('.acc_navigation_bar').removeClass('hidden_offscreen');
        }
      }
    });
  });
</script>
<!--HEADERINFO SOLVE SCRIPT-->
<script async>
  require(['jquery', 'jquery/ui'], function ($) {
    $(document).ready(function () {
      let pageTopPadding = parseInt($('.page-wrapper').css('padding-top'), 10);
      console.log('Top padding of the page is:' + pageTopPadding);
      let navigationTopOffset = pageTopPadding - 16;
      console.log('Top padding of the page is:' + navigationTopOffset);
      $('body')
        .get(0)
        .style.setProperty('--nav-position', navigationTopOffset + 'px');
      // $(".acc_navigation_bar").css("top", navigationTopOffset);
    });

    $('.message-info__close').click(function (resetAccessoryNavigationTopOffset) {
      $('body').get(0).style.setProperty('--nav-position', '64px');
    });
  });
</script>
