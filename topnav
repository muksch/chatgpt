<script>
  require(['jquery', 'jquery/ui'], function ($) {
    $(document).ready(function assignHeaderOne() {
      let currentCategoryName = $('h1.page-title').attr('title');
      $('.current_category h1').html(currentCategoryName);

      setTimeout(function () {
        let currentCategoryName = $('h1.page-title').attr('title');
        $('.current_category h1').html(currentCategoryName);
      }, 500);
    });
  });
</script>
<!-- PLACE NAVIGATION TO THE RIGHT SPOT IN HTML STRUCTURE -->
<script>
  require(['jquery', 'jquery/ui'], function ($) {
    // $('.acc_navigation_container').prependTo( $('.page-main') ); // Moves the acc navigation to the top
    $('.breadcrumbs').clone().appendTo($('.acc_breadcrumbs'));
    //if ($(window).width() > 767) {
    $('.acc_navigation_container').clone().prependTo($('#maincontent')).attr('class', 'acc_navigation_container_fixed');
    $('.acc_navigation_header').prependTo($('#maincontent'));
    //}

    var current = window.location.pathname; // Highlights the active category in dropdown
    $('.nav_by_category a').each(function () {
      var link = $(this).attr('href');
      if (current === link) {
        $(this).addClass('active');
      }
    });
  });
</script>

